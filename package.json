{
  "name": "rain-util-postgres",
  "version": "0.3.8",
  "author": "Max Millien",
  "description": "Generator based, co/koa compatible utility for accessing postgres",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/rain-util-postgres.git"
  },
  "license": "MIT",
  "main": "./bin",
  "scripts": {
    "clean": "npm cache clean && rm -rf node_modules bin data",
    "build": "babel lib -d bin",
    "test": "tape ./test"
  },
  "keywords": [
    "koa",
    "postgres",
    "utils",
    "pg",
    "sql",
    "dao",
    "data access",
    "generators",
    "bundle",
    "convenience",
    "toolkit"
  ],
  "dependencies": {
    "pg": "4.4.x",
    "postgres-gen": "0.x",
    "postgres-gen-dao": "0.x"
  },
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-3": "*",
    "co-tape": "*",
    "tape": "4.x",
    "tape-catch": "1.x"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "babel": {
    "presets": [
      "es2015-node5",
      "stage-3"
    ]
  }
}
